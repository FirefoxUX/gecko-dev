{
  "border": {
    "color": {
      "prefers-contrast": "{text.color}"
    },
    "interactive": {
      "color": {
        "forced-colors": {
          "value": "ButtonText",
          "active": "ButtonText",
          "hover": "SelectedItem"
        },
        "prefers-contrast": {
          "value": "AccentColor",
          "active": "ButtonText",
          "disabled": "GrayText",
          "hover": "AccentColor"
        },
        "brand": {
          "light": "{color.gray.60}",
          "dark": "{color.gray.50}"
        },
        "platform": "color-mix(in srgb, currentColor 15%, {color.gray.60})"
      }
    },
    "radius": {
      "circle": "9999px",
      "small": "4px"
    },
    "width": "1px"
  },
  "box": {
    "background": {
      "color": {
        "light": "{color.white}",
        "dark": "{color.gray.80}",
        "prefers-contrast": "{color.canvas}"
      }
    },
    "shadow": {
      "10": {
        "type": "shadow",
        "value": {
          "color": "{color.black.a10}",
          "offsetX": "0",
          "offsetY": "1px",
          "blur": "4px",
          "spread": "0"
        }
      }
    }
  },
  "button": {
    "background": {
      "color": {
        "brand": {
          "value": "color-mix(in srgb, currentColor 7%, transparent)",
          "hover": "color-mix(in srgb, currentColor 14%, transparent)",
          "active": "color-mix(in srgb, currentColor 21%, transparent)"
        },
        "platform": {
          "value": "var(--button-bgcolor)",
          "hover": "var(--button-hover-bgcolor)",
          "active": "var(--button-active-bgcolor)"
        }
      }
    },
    "border": {
      "radius": "{border.radius.small}"
    },
    "font-weight": "{fontWeight.bold}",
    "line-height": "{lineHeight.small}",
    "min-height": "{size.item.large}"
  },
  "canvas": {
    "color": {
      "prefers-contrast": "Canvas",
      "brand": {
        "light": "{color.white}",
        "dark": "{color.gray.90}"
      },
      "platform": "Canvas"
    }
  },
  "color": {
    "accent": {
      "primary": {
        "forced-colors": {
          "value": "ButtonText",
          "active": "ButtonText",
          "hover": "SelectedItem"
        },
        "prefers-contrast": {
          "value": "AccentColor",
          "active": "ButtonText",
          "hover": "ButtonText"
        },
        "brand": {
          "light": {
            "value": "{color.blue.50}",
            "active": "{color.blue.70}",
            "hover": "{color.blue.60}"
          },
          "dark": {
            "value": "{color.cyan.50}",
            "active": "{color.cyan.20}",
            "hover": "{color.cyan.30}"
          }
        },
        "platform": {
          "value": "var({button.primary.bgcolor}, AccentColor})",
          "active": "var({button.primary.active.bgcolor})",
          "hover": "var({button.primary.hover.bgcolor})"
        }
      }
    },
    "background": {
      "critical": {
        "light": "{color.red,05}",
        "dark": "{color.red.80}",
        "prefers-contrast": false
      },
      "information": {
        "light": "{color.blue.05}",
        "dark": "{color.blue.80}",
        "prefers-contrast": false
      },
      "success": {
        "light": "{color.green.05}",
        "dark": "{color.yellow.80}",
        "prefers-contrast": false
      },
      "warning": {
        "light": "{color.yellow.05}",
        "dark": "{color.yellow.80}",
        "prefers-contrast": false
      }
    },
    "black": {
      "a10": {
        "value": "rgba(0,0,0,0.1)"
      }
    },
    "blue": {
      "05": {
        "value": "#deeafc"
      },
      "30": {
        "value": "#73a7f3"
      },
      "50": {
        "value": "#0060df"
      },
      "60": {
        "value": "#0250bb"
      },
      "70": {
        "value": "#054096"
      },
      "80": {
        "value": "#003070"
      }
    },
    "cyan": {
      "20": {
        "value": "#aaf2ff"
      },
      "30": {
        "value": "#aaf2ff"
      },
      "50": {
        "value": "00ddff"
      }
    },
    "gray": {
      "05": {
        "value": "#fbfbfe"
      },
      "50": {
        "value": "#bfbfc9"
      },
      "60": {
        "value": "#8f8f9d"
      },
      "70": {
        "value": "#5b5b66"
      },
      "80": {
        "value": "#23222b"
      },
      "90": {
        "value": "#1c1b22"
      },
      "100": {
        "value": "#15141a"
      }
    },
    "green": {
      "05": {
        "value": "#d8eedc"
      },
      "30": {
        "value": "#4dbc87"
      },
      "50": {
        "value": "#017a40"
      },
      "80": {
        "value": "#004725"
      }
    },
    "red": {
      "05": {
        "value": "#ffe8e8"
      },
      "30": {
        "value": "#f37f98"
      },
      "50": {
        "value": "#d7264c"
      },
      "80": {
        "value": "#690f22"
      }
    },
    "yellow": {
      "05": {
        "value": "#ffebcd"
      },
      "30": {
        "value": "#e49c49"
      },
      "50": {
        "value": "#cd411e"
      },
      "80": {
        "value": "#5a3100"
      }
    },
    "white": "#ffffff"
  },
  "focus-outline": {
    "color": "{color.accent.primary}",
    "offset": "2px",
    "width": "2px",
    "type": "outline"
  },
  "icon": {
    "color": {
      "value": {
        "light": "{color.gray.70}",
        "dark": "{color.gray.05}"
      },
      "critical": {
        "light": "{color.red.50}",
        "dark": "{color.red.30}",
        "prefers-contrast": false
      },
      "information": {
        "light": "{color.blue.50}",
        "dark": "{color.blue.30}",
        "prefers-contrast": false
      },
      "success": {
        "light": "{color.green.50}",
        "dark": "{color.green.30}",
        "prefers-contrast": false
      },
      "warning": {
        "light": "{color.yellow.50}",
        "dark": "{color.yellow.30}",
        "prefers-contrast": false
      }
    }
  },
  "input": {
    "text": {
      "line-height": "{button.lineHeight}",
      "min-height": "{button.minHeight}"
    }
  },
  "font-size": {
    "brand": {
      "root": "15px",
      "small": "0.867rem",
      "large": "1.133rem",
      "xlarge": "1.467rem",
      "xxlarge": "2.2rem"
    },
    "platform": {
      "root": "unset",
      "small": "unset",
      "large": "unset",
      "xlarge": "unset",
      "xxlarge": "unset"
    }
  },
  "font-weight": {
    "default": "normal",
    "bold": "600",
    "brand": {
      "light": "300"
    },
    "platform": {
      "light": "unset"
    }
  },
  "line-height": {
    "brand": {
      "default": "1.5",
      "small": "1.3"
    },
    "platform": {
      "default": "normal",
      "small": "unset"
    }
  },
  "link": {
    "color": {
      "prefers-contrast": {
        "value": "LinkText",
        "hover": "LinkText",
        "active": "ActiveText",
        "visited": "{link.color}"
      },
      "brand": {
        "value": "{color.accent.primary}",
        "hover": "{brand.color.accent.primary.hover}",
        "active": "{brand.color.accent.primary.active}",
        "visited": "{link.color}"
      },
      "platform": {
        "value": "LinkText",
        "hover": "LinkText",
        "active": "ActiveText",
        "visited": "{link.color}"
      }
    },
    "focus-outline": {
      "offset": "1px"
    }
  },
  "size": {
    "item": {
      "large": "32px",
      "small": "16px"
    }
  },
  "text": {
    "color": {
      "prefers-contrast": {
        "value": "CanvasText",
        "deemphasized": "inherit"
      },
      "brand": {
        "default": {
          "light": "{color.gray.100}",
          "dark": "{color.gray.05}"
        },
        "deemphasized": {
          "light": "color-mix(in srgb, currentColor 69%, transparent)",
          "dark": "color-mix(in srgb, currentColor 75%, transparent)"
        }
      },
      "platform": {
        "default": "currentColor",
        "deemphasized": "color-mix(in srgb, currentColor 60%, transparent)"
      }
    }
  }
}
